<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="385.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.control.ControladorListarReservasSucursal">
   <children>
      <BorderPane prefHeight="385.0" prefWidth="624.0">
         <top>
            <HBox prefHeight="54.0" prefWidth="286.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Introduce Sucursal" />
                  <TextField fx:id="txtSuc" prefHeight="25.0" prefWidth="92.0" />
                  <Button fx:id="buscar" mnemonicParsing="false" onAction="#buscar" text="Buscar" />
               </children>
            </HBox>
         </top>
         <center>
            <TableView fx:id="tvReserva" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="tcId" prefWidth="47.0" text="ID" />
                <TableColumn fx:id="tcFechaRec" prefWidth="95.0" text="FECHAREC" />
                  <TableColumn fx:id="tcFechaDev" prefWidth="118.0" text="FECHADEV" />
                  <TableColumn fx:id="tcSucDev" prefWidth="67.0" text="SUCDEV" />
                  <TableColumn fx:id="tcModAlq" prefWidth="124.0" text="MODALQ" />
                  <TableColumn fx:id="tcCliente" prefWidth="58.0" text="CLIENTE" />
                  <TableColumn fx:id="tcCat" prefWidth="151.0" text="CATEGORIA" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
